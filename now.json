{
  "name": "hon-avatar",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "regions": ["pdx1"],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "cache-control": "s-maxage=86400",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
