{
  "name": "hon-avatar",
  "version": 2,
  "regions": ["pdx1"],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js",
      "headers": {
        "cache-control": "s-maxage=86400",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "functions": {
    "api/index.js": {
      "memory": 128,
      "maxDuration": 3
    }
  }
}
